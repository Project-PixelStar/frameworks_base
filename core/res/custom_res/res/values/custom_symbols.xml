<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Package Manager -->
    <java-symbol type="array" name="config_deviceDisabledComponents" />
    <java-symbol type="array" name="config_globallyDisabledComponents" />
    <java-symbol type="array" name="config_forceEnabledComponents" />

    <!-- Leds -->
    <java-symbol type="bool" name="config_intrusiveBatteryLed" />

    <!-- Device keyhandlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- Camera -->
    <java-symbol type="array" name="config_cameraHFRPrivAppList" />
    <java-symbol type="array" name="config_cameraAuxPackageAllowList" />
    <java-symbol type="array" name="config_cameraAuxPackageExcludeList" />

    <!-- Whether to cleanup fingerprints upon connection to the daemon and when
         user switches -->
    <java-symbol type="bool" name="config_cleanupUnusedFingerprints" />

    <!-- Whether to cancel fingerprint operation if not idle -->
    <java-symbol type="bool" name="config_fpCancelIfNotIdle" />

    <!-- Location indicator -->
    <java-symbol type="drawable" name="stat_sys_location" />

    <!-- For dynamic default font styles -->
    <java-symbol type="string" name="config_bodyFontFamily" />

    <!-- Alert Slider -->
    <java-symbol type="string" name="alert_slider_state_path" />
    <java-symbol type="string" name="alert_slider_uevent_match_path" />
    <java-symbol type="bool" name="config_hasAlertSlider" />

    <!-- These are system packages exempted from privacy indicator -->
    <java-symbol type="array" name="config_indicatorExemptedPackageNames" />

    <!-- Should we listen for fingerprints when the screen is off?  Devices
         with a rear-mounted sensor want this, but certain devices have
         the sensor embedded in the power key and listening all the time
         causes a poor experience. -->
    <java-symbol type="bool" name="config_fingerprintWakeAndUnlock" />

    <!-- Whether to force enable multi resolution configuration for camera -->
    <java-symbol type="bool" name="config_forceMultiResolution" />
</resources>
